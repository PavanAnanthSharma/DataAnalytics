ontract(jsonInterface, address);\ncontract.methods.somFunc().send({from: ....})\n.on('receipt', function(){\n    ...\n});\n```\n\n\n[docs]: http://web3js.readthedocs.io/en/1.0/\n[repo]: https://github.com/ethereum/web3.js\n\n\n","maintainers":[{"name":"frozeman","email":"fabian@frozeman.de"}],"time":{"modified":"2018-08-27T06:16:05.505Z","created":"2017-05-21T15:16:42.353Z","1.0.9":"2017-05-21T15:16:42.353Z","1.1.0":"2017-05-21T15:27:52.077Z","1.0.0-beta.8":"2017-07-24T14:08:19.573Z","1.0.0-beta.9":"2017-07-24T16:49:46.523Z","1.0.0-beta.10":"2017-07-24T17:07:23.147Z","1.0.0-beta.11":"2017-07-24T17:47:50.055Z","1.0.0-beta.12":"2017-07-26T17:24:25.888Z","1.0.0-beta.13":"2017-07-27T13:49:49.804Z","1.0.0-beta.14":"2017-08-02T13:26:20.607Z","1.0.0-beta.15":"2017-08-03T11:44:07.809Z","1.0.0-beta.16":"2017-08-07T16:09:21.357Z","1.0.0-beta.17":"2017-08-08T10:24:37.374Z","1.0.0-beta.18":"2017-08-09T14:17:33.937Z","1.0.0-beta.19":"2017-09-15T11:54:39.878Z","1.0.0-beta.20":"2017-09-15T11:58:57.397Z","1.0.0-beta.21":"2017-09-20T13:54:04.633Z","1.0.0-beta.22":"2017-09-25T15:06:15.206Z","1.0.0-beta.23":"2017-10-10T11:24:41.566Z","1.0.0-beta.24":"2017-10-19T13:39:13.730Z","1.0.0-beta.25":"2017-11-14T11:15:53.543Z","1.0.0-beta.26":"2017-11-14T11:23:45.982Z","1.0.0-beta.27":"2017-12-21T11:09:10.140Z","1.0.0-beta.28":"2018-01-17T17:06:59.067Z","1.0.0-beta.29":"2018-01-26T09:39:22.074Z","1.0.0-beta.30":"2018-02-13T11:33:36.031Z","1.0.0-beta.31":"2018-03-13T14:06:37.909Z","1.0.0-beta.32":"2018-03-19T20:03:34.273Z","1.0.0-beta.33":"2018-03-19T20:22:49.523Z","1.0.0-beta.34":"2018-04-13T14:57:01.904Z","1.0.0-beta.35":"2018-07-25T13:26:38.314Z","1.0.0-beta.36":"2018-08-27T06:15:58.926Z"},"repository":{"type":"git","url":"https://github.com/ethereum/web3.js/tree/master/packages/web3-eth-contract"},"license":"LGPL-3.0","readmeFilename":"README.md","_attachments":{},"_etag":"W/\"43c1ffe709ea70429a50a55577797773\"","_lastModified":"Mon, 27 Aug 2018 06:16:06 GMT"}